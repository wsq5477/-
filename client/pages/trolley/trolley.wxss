.order{
  position: relative;
  top:-150rpx;
   background: #fff;
   border-radius: 13rpx;
   box-shadow: 0 2rpx 13rpx 5rpx rgba(0, 0, 0, 0.02);
   margin:0rpx 30rpx 30rpx 30rpx;
}
.trolley-card{
  display:flex;
  position:relative;
  top:-180rpx;
  margin-left:60rpx;
}
.trolley-text,.edit{
   color: rgb(52, 55, 61, 0.8);
  font-size: 24rpx;
  line-height: 33rpx;
}
.edit{
  margin-left:520rpx;
}
.trolley-check-wrapper{
  border-radius:50% 50%;
  margin-right:20rpx;
  width:22rpx;
  height:16rpx;
  padding:8rpx 6rpx;
}
.trolley-check{
  width:22rpx;
  height:16rpx;
  padding-bottom:50rpx;
}
.trolley-check-wrapper.white{
  background:#fff;
}
.trolley-check-wrapper.check {
 background: #F5E069;
}
.orderList{
  display:flex;
  align-items: center;
  border-bottom:1px solid rgba(151, 151, 151, 0.2);
  margin:0rpx 30rpx;
}
.check{
  width:22rpx;
  height:16rpx;
  background:#ececee;
  border-radius:50% 50%;
  padding:10rpx 8rpx;
  margin-right:20rpx;
}
.orderList:last-child{
  border-bottom:none;
}
.order-image{
  width:200rpx;
  height:200rpx;
  margin:20rpx 20rpx 20rpx 0rpx;
}
.order-product{
  margin-top:-40rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  box-sizing: border-box;
  width:300rpx;
}
.order-name{
   color: rgba(29, 29, 38, 0.8);
}
.order-price{
  margin-top: 12rpx;
  font-weight: bold;
}
.evoluate-number{
  font-size:28rpx;
  font-weight: bold;
  text-align: right;
  padding-top:90rpx;
}
.evoluate-count{
  display:flex;
  align-items: center;
}
.evoluate-division,.evoluate-sum{
  font-size:28rpx;
  border:1px solid #F6E474;
  border-radius:50% 50%;
  margin-top:90rpx;
  width:36rpx;
  height:36rpx;
  line-height:36rpx;
  text-align:center;
  font-weight:900;
  margin-left:20rpx;
  margin-right:20rpx;
}
.trolley-list{
  display:flex;
  padding-left:60rpx;
  position:fixed;
  bottom:0;
  left:0;
  right:0;
  align-items: center;
  background:#ececee;
}
.trolley-account{
  color: rgba(52, 55, 61, 0.8);
  font-size: 24rpx;
  line-height: 33rpx;
  margin-left:200rpx;
  margin-right:20rpx;
}
.account{
  font-size: 34rpx;
  line-height: 48rpx;
  margin-right:20rpx;
}
.trolley-btn{
  flex:1;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  height: 100%;
  background: #F5E069;
  color: #34373D;
}
.trolley-btn.forbid{
  color: rgba(52, 55, 61, 0.5);
}
.order-empty{
   background:#fff;
   position:relative;
   top:-150rpx;
   box-shadow: 0 2rpx 13rpx 5rpx rgba(0, 0, 0, 0.02);
   border-radius:13rpx;
   display: flex;
   flex-direction:column;
   justify-content: center;
   align-items: center;
   height:431rpx;
   margin:0rpx 30rpx;
}
.empty-image{
  width:80rpx;
  height:80rpx;
}
.empty-text{
  font-size: 28rpx;
  line-height: 40rpx;
  color: rgba(139, 139, 139, 0.8);
}